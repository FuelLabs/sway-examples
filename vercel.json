{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "installCommand": "pnpm install",
    "buildCommand": "pnpm build:preview",
    "devCommand": "",
    "outputDirectory": "dist",
    "cleanUrls": false,
    "trailingSlash": true,
    "rewrites": [
      {
        "source": "/:path*",
        "destination": "/nft/:path*",
        "has": [
          {
            "type": "host",
            "value": "sway-nft.vercel.app"
          }
        ]
      },
      {
        "source": "/nft/:path*",
        "destination": "/nft/create/:path*",
        "has": [
          {
            "type": "host",
            "value": "sway-nft.vercel.app"
          }
        ]
      }
    ]
}
