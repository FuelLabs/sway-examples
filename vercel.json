{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "installCommand": "pnpm install",
    "buildCommand": "pnpm build:preview",
    "devCommand": "",
    "outputDirectory": "dist",
    "cleanUrls": false,
    "trailingSlash": true,
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "https://sway-examples.vercel.app/nft$1",
        "has": [
          {
            "type": "host",
            "value": "sway-nft.vercel.app"
          }
        ]
      }, {
        "source": "/:path*",
        "destination": "https://sway-examples.vercel.app/nft/:path",
        "has": [
          {
            "type": "host",
            "value": "sway-nft.vercel.app"
          }
        ]
      }
    ]
}
