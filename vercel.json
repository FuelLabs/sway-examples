{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "installCommand": "pnpm install",
  "buildCommand": "pnpm build:preview",
  "outputDirectory": "./dist",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/nft/$1",
      "has": [
        {
          "type": "host",
          "value": "sway-nft.vercel.app"
        }
      ]
    }
  ]
}
